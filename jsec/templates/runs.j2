{% extends "base.j2" %}
{% block body %}
<div class="row">
    <div class="col-lg-5">
        {% include "analysis-details.j2" %}
    </div>
    <div class="col-lg-7">
        {% with card = analysis['card'] %}
            {% include "card.j2" %}
        {% endwith %}
    </div>
</div>
<div class="row">
    <div class="col-lg-12">Message: {{ analysis["message"] }}</div>
</div>
<div class="row">
    <div class="col-lg-5">
        {% include "legend.j2" %}
    </div>
</div>
{% set attacks = analysis["analysis-results"] %}
{%  include "run-attacks.js2" %}
{% endblock %}
